{
  "name": "furlong",
  "version": "0.2.3",
  "description": "A Typescript/JS/node library for computing pairwise distance metrics",
  "author": "Seth Brown <software@seth-brown.net>",
  "keywords": [
    "statistics",
    "distance",
    "pairwise",
    "euclidean",
    "manhattan",
    "braycurtis",
    "chebyshev",
    "canberra"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prepublish": "npm run build",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.3.0",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3",
    "@types/node": "^13.11.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seth-brown/furlong.git"
  },
  "licenses": [
    {
      "type": "BSD",
      "url": "https://github.com/seth-brown/furlong/blob/master/LICENSE"
    }
  ],
  "publishConfig": {
    "access": "public"
  }
}
